
{{#if user}}
    <div class="container">
        <table class="table table-striped table-info">
            <thead>
                <tr>
                <th scope="col">#</th>
                <th scope="col">Username</th>
                <th scope="col">Email</th>
                <th scope="col">Watch Count</th>
                </tr>
            </thead>
            <tbody>
                {{#each users}}
                <tr>
                <th scope="row">{{@index}}</th>
                <td>{{username}}</td>
                <td>{{email}}</td>
                <td>{{this.activity.length}}</td>
                </tr>
                {{/each}}
            </tbody>
        </table>
            
       
    </div>
        
        
        
{{else}}
     <div class="alert alert-danger">Please login</div>
{{/if}}

